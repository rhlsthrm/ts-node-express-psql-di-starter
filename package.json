  "bugs": {
    "url": "https://github.com/ConnextProject/cf-hub-experimental/issues"
  },
  "homepage": "https://github.com/ConnextProject/cf-hub-experimental#readme",
  "dependencies": {
    "bignumber.js": "^8.1.1",
    "connect-redis": "^3.4.1",
    "cookie-parser": "^1.4.4",
    "ethers": "^4.0.27",
    "express": "^4.16.4",
    "express-promise-router": "^3.0.3",
    "express-session": "^1.15.6",
    "handy-redis": "^1.5.2",
    "logdna": "^3.0.1",
    "path-to-regexp": "^3.0.0",
    "pg": "^7.9.0",
    "sql-template-strings": "^2.2.2",
    "web3": "^1.0.0-beta.52"
  },
  "devDependencies": {
    "@spankchain/chai-subset": "^1.7.0",
