    "@types/node": "^11.13.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "db-migrate": "^0.11.5",
    "db-migrate-pg": "^0.5.0",
    "fetch-ponyfill": "^6.0.2",
    "mocha": "^6.1.2",
    "nock": "^10.0.6",
    "nodemon": "^1.18.11",
    "sinon": "^7.3.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.2"
  }
